<body>
<div class="wrapper">
    <!-- @import "kit/header.kit" -->

    <div class="content">
        <div class="slider">
            <div class="slider__item">
                <div class="slide__bg"><div class="shadow"></div><img src="src/img/slider-1.jpg" alt=""></div>
                <div class="slide__text">
                    <div class="slide__title">
                        <span>Design & Branding</span>
                    </div>
                    <div class="slide__subtitle">
                        We Deliver Quality Results
                    </div>
                </div>
            </div>
            <div class="slider__item">
                <div class="slide__bg"><div class="shadow"></div><img src="src/img/slider-2.jpg" alt=""></div>
                <div class="slide__text">
                    <div class="slide__title">
                        <span>Lian Creative Agency</span>
                    </div>
                    <div class="slide__subtitle">
                        Minimal Freelance Portfolio
                    </div>
                </div>
            </div>
            <div class="slider__item">
                <div class="slide__bg"><div class="shadow"></div><img src="src/img/slider-1.jpg" alt=""></div>
                <div class="slide__text">
                    <div class="slide__title">
                        <span>Design & Branding</span>
                    </div>
                    <div class="slide__subtitle">
                        We Deliver Quality Results
                    </div>
                </div>
            </div>
            <div class="slider__item">
                <div class="slide__bg"><div class="shadow"></div><img src="src/img/slider-2.jpg" alt=""></div>
